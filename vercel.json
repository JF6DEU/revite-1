{
    "rewrites": [
        {
            "source": "/(.*)",
            "has": [
                {
                    "type": "cookie",
                    "key": "gizou",
                    "value": "OK"
                }
            ],
            "destination": "/spf.html"
        }
    ],
    "redirects": [
        {
            "source": "/(.*)",
            "missing": [
                {
                    "type": "cookie",
                    "key": "gizou",
                    "value": "OK"
                },
                {
                    "type": "query",
                    "key": "redirected"
                }
            ],
            "destination": "/index.html?redirected"
        }
    ]
}
